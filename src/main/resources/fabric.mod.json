{
  "schemaVersion": 1,
  "id": "villagernames",
  "version": "${version}",
  "name": "Villager Names",
  "description": "A mod that adds names to villagers",
  "authors": [
    "Victor (Yumatan)",
    "OverlordsIII",
    "Fourmisain"
  ],
  "entrypoints": {
    "main": [
      "io.github.overlordsiii.villagernames.VillagerNames"
    ],
    "client": [
      "io.github.overlordsiii.villagernames.client.VillagerNamesClient"
    ],
    "modmenu": [
      "io.github.overlordsiii.villagernames.integration.ModMenuIntegration"
    ],
    "cardinal-components-level": [
      "io.github.overlordsiii.villagernames.VillagerNames"
    ]
  },
  "license": "MIT",
  "icon": "assets/villagernames/villager_names_fork_icon.png",
  "environment": "*",
  "mixins": [
    "villagernames.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.16.14",
    "fabric-api": "*",
    "java": ">=21",
    "minecraft": "~1.21.6"
  },
  "custom": {
    "cardinal-components": [
      "villagernames:intcomponent"
    ]
  }
}
